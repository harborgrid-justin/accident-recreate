{
  "name": "accuscene-enterprise",
  "version": "0.2.0",
  "description": "AccuScene Enterprise Accident Recreation Platform",
  "main": "./dist/main/main.js",
  "author": "AccuScene Enterprise",
  "license": "PROPRIETARY",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development electron .",
    "dev": "concurrently \"npm run dev:renderer\" \"npm run dev:api\" \"npm run dev:electron\"",
    "dev:renderer": "webpack serve --config webpack.renderer.config.js --mode development",
    "dev:api": "nodemon --watch src/api --exec ts-node src/api/server.ts",
    "dev:electron": "webpack --config webpack.main.config.js --mode development && cross-env NODE_ENV=development electron .",
    "build": "npm run build:renderer && npm run build:main && npm run build:api",
    "build:renderer": "webpack --config webpack.renderer.config.js --mode production",
    "build:main": "webpack --config webpack.main.config.js --mode production",
    "build:api": "tsc --project tsconfig.api.json",
    "package": "electron-builder",
    "package:win": "electron-builder --win",
    "package:mac": "electron-builder --mac",
    "package:linux": "electron-builder --linux",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,scss,md}\"",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist build"
  },
  "dependencies": {
    "@apollo/server": "^4.10.0",
    "@apollo/subgraph": "^2.7.0",
    "@react-three/drei": "^9.96.0",
    "@react-three/fiber": "^8.15.0",
    "@use-gesture/react": "^10.3.0",
    "bcrypt": "^5.1.1",
    "better-sqlite3": "^9.2.2",
    "canvas": "^2.11.2",
    "cors": "^2.8.5",
    "dataloader": "^2.2.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "fabric": "^5.3.0",
    "graphql": "^16.8.1",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tag": "^2.12.6",
    "helmet": "^7.1.0",
    "json-schema": "^0.4.0",
    "jsonwebtoken": "^9.0.2",
    "konva": "^9.3.0",
    "pdfkit": "^0.13.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-konva": "^18.2.10",
    "react-router-dom": "^6.21.1",
    "recharts": "^2.12.0",
    "reflect-metadata": "^0.2.1",
    "socket.io": "^4.7.0",
    "socket.io-client": "^4.7.0",
    "sqlite3": "^5.1.7",
    "three": "^0.161.0",
    "typeorm": "^0.3.19",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.16.0",
    "yjs": "^13.6.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/better-sqlite3": "^7.6.8",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/json-schema": "^7.0.15",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.10.6",
    "@types/pdfkit": "^0.13.4",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@types/three": "^0.161.0",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-node-externals": "^3.0.0"
  },
  "build": {
    "appId": "com.accuscene.enterprise",
    "productName": "AccuScene Enterprise",
    "directories": {
      "output": "build",
      "buildResources": "assets"
    },
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "icon": "assets/icon.ico"
    },
    "mac": {
      "target": [
        "dmg",
        "zip"
      ],
      "icon": "assets/icon.icns",
      "category": "public.app-category.business"
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ],
      "icon": "assets/icon.png",
      "category": "Office"
    }
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src"
    ],
    "testMatch": [
      "**/__tests__/**/*.ts",
      "**/?(*.)+(spec|test).ts"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/**/*.d.ts",
      "!src/**/*.test.ts",
      "!src/**/*.spec.ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^@main/(.*)$": "<rootDir>/src/main/$1",
      "^@renderer/(.*)$": "<rootDir>/src/renderer/$1",
      "^@api/(.*)$": "<rootDir>/src/api/$1",
      "^@database/(.*)$": "<rootDir>/src/database/$1",
      "^@physics/(.*)$": "<rootDir>/src/physics/$1",
      "^@editor/(.*)$": "<rootDir>/src/editor/$1",
      "^@reports/(.*)$": "<rootDir>/src/reports/$1",
      "^@types/(.*)$": "<rootDir>/src/types/$1",
      "^@utils/(.*)$": "<rootDir>/src/utils/$1"
    }
  }
}
